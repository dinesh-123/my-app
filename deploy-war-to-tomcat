node{
   stage('SCM Checkout'){
     git 'https://github.com/dinesh-123/my-app.git'
   }
   stage('Compile-Package'){
      // Get maven home path
      sh "/usr/share/maven/bin/mvn package"
   }
   stage('Deploy to Tomcat'){
   
      sshagent(['tomcat-dev']) {
         sh 'scp -o StrictHostKeyChecking=no /var/lib/jenkins/workspace/Groovy Job/target ubuntu@172.31.41.107:/var/lib/tomcat8/webapps/'
      }
    }
  }
  
